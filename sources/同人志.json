{
    "name": "同人志",
    "charset": "UTF-8",
    "search": {
        "requestURL": "https://m.c-upp.com/search/",
        "requestMethod": "POST",
        "requestData": "show=title%2Ctitleen%2Ctags&keyboard=[$key]"
    },
    "metadata": {
        "list": "{selector:div > ul@start->1}",
        "name": "{selector:h3}",
        "author": "{selector:a > p}",
        "img": "{selector:img@attr->src}",
        "link": "{selector:a}"
    },
    "detail": {
        "author": "{selector:}",
        "summary": "{selector:}",
        "upDate": "{selector:}",
        "catalog_link": [],
        "catalog": {
            "chapter": {
                "list": "{selector:div > a}",
                "name": "{selector:a}",
                "link": "{selector:a@attr->href}",
                "orderBy": 0,
                "category": 1
            }
        }
    },
    "content": {
        "list": "{sub:[$html],initData(,);,0}{selector:page_url}",
        "link": "{selector:}",
        "mode": 2
    }
}