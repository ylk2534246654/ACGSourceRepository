{
  "name": "嘀哩嘀哩",
  "charset": "gb2312",
  "search": {
    "requestURL": "http://www.dilidili.one/search.php?&keyword=[$key]&PageNo=[$page]",
	"requestMethod": "GET",
    "requestData": ""
  },"metadata": {
    "list": "{selector:div > div > div:nth-child(3) > div}",
    "name": "{selector:div > div > a > h2[class=post-title]}",
    "author": "{selector:p.slr}",
    "img": "{selector:div > div > a > img@attr->src}",
	"link": "{selector:div > div > a@attr->href}{re:dilidili.name,dilidili.one}{re:www.dilidili,m.dilidili}"
  },
  "detail": {
    "author": "",
    "summary": "{selector:#app > div:nth-child(3) > p}",
    "upDate": "{selector:#app > div.episode > div}{sub:[$this],剧集（,）,1}",
    "catalog":"[#如果目录在其他页面,则添加跳转链接的获取]",
	"chapter": [{
		"list": "{selector:div.episode > ul > li}",
		"name": "{selector:a}",
		"link": "{selector:a@attr->href}",
		"orderBy": 1,
		"category":3
	  }]
  }
}