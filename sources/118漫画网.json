{
  "name": "118漫画网",
  "charset": "UTF-8",
  "search": {
    "requestURL": "http://m.ccshwy.com/statics/search.aspx?key=[$key]",
    "requestMethod": "GET"
  },
  "metadata": {
    "list": "{selector:#listbody > li}",
    "name": "{selector:a.txtA}",
    "author": "{selector:span.info}",
    "img": "{selector:a.ImgA > img@attr->src}",
    "link": "{selector:a.ImgA@attr->href}"
  },
  "detail": {
    "author": "{selector:div.sub_r > p:nth-child(2)}",
    "summary": "{selector:p.txtDesc}{re:介绍:,}",
    "upDate": "{selector:span.date}",
    "catalog_link": [],
    "catalog": {
      "list": [],
      "chapter": {
        "list": "{selector:div#list > ul > li}",
        "name": "{selector:a}",
        "link": "{selector:a@attr->href}",
        "orderBy": 1,
        "category": 2
      }
    }
  },
  "content": {
    "备注": "此处爬取数据中的指定json数据,再json解析出link",
    "list": "{sub:[$html],m_murl_e=\",\",0}{selector:}",
    "link": "{sub:[$html],z_yurl=',',0}{selector:}",
    "mode": 2
  }
}