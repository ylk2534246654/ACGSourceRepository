{
  "name": "布卡漫画",
  "introduction": "可以搜索一些漫画",
  "developer": "雨夏",
  "mail":"2534246654@qq.com",
  "versionName": "1.1",
  "versionCode": 3,
  "tab":"漫画",
  "request": "http://m.buka.cn/search/ajax_search@post->count=15&key={key}&start={page}@charset->UTF-8",
  "search": {
    "list": "function Main(html){return jsonPaths(html,'$.datas.items[*]');}",
    "title": "function Main(text){return jsonPath(text,'$.name');}",
    "introduction": "function Main(text){return jsonPath(text,'$.author');}",
    "cover": "function Main(text){return jsonPath(text,'$.logo');}",
    "url": "function Main(text){return 'http://www.buka.cn/detail/'+jsonPath(text,'$.mid');}"
  },
  "detail": {
    "author": "function Main(html){return jsoup(html,'div.manga-author > a');}",
    "introduction": "function Main(html){return jsoup(html,'div.manga-desc > p');}",
    "upDate": "function Main(html){return jsoup(html,'span.manga-episodes_text').replace(':','');}",
    "tabs": {
      "list": "function Main(html){return jsoups(html,'div.manga-episodes_title');}",
      "name": "function Main(text){return jsoup(text,'span.manga-episodes_text');}"
    },
    "catalogs": {
      "list": "function Main(html){return jsoups(html,'div.manga-episodes');}",
      "chapter": {
        "list": "function Main(html){return jsoups(html,'div.epsbox > a');}",
        "name": "function Main(text){return jsoup(text,'a');}",
        "url": "function Main(text){return jsoup(text,'a@attr->href');}",
        "category": 2
      }
    }
  },
  "comic": {
    "list": "function Main(html){var imgs = jsoups(html,'#manga-imgs > div.manga-c');return imgs;}",
    "url": "function Main(text){var img = jsoup(text,'img@attr->data-original');if(img.indexOf('.jpg')==-1){img=jsoup(text,'img@attr->src')};return img}",
    "mode": 3
  }
}