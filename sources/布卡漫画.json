{
  "name": "布卡漫画",
  "charset": "UTF-8",
  "request": "http://m.buka.cn/search/ajax_search@post->count=15&key=[$key]&start=[$page]",
  "metadata": {
    "list": "function Main(html){return jsonPaths(html,'$.datas.items[*]');}",
    "name": "function Main(text){return jsonPath(text,'$.name');}",
    "author": "function Main(text){return jsonPath(text,'$.author');}",
    "img": "function Main(text){return jsonPath(text,'$.logo');}",
    "link": "function Main(text){return 'http://www.buka.cn/detail/'+jsonPath(text,'$.mid');}"
  },
  "detail": {
    "author": "function Main(html){return jsoup(html,'div.manga-author > a');}",
    "summary": "function Main(html){return jsoup(html,'div.manga-desc > p');}",
    "upDate": "function Main(html){return jsoup(html,'span.manga-episodes_text').replace(':','');}",
    "catalog_link": [],
    "tab": {
      "list": "function Main(html){return jsoups(html,'div.manga-episodes_title');}",
      "name": "function Main(text){return jsoup(text,'span.manga-episodes_text');}"
    },
    "catalog": {
      "list": "function Main(html){return jsoups(html,'div.manga-episodes');}",
      "chapter": {
        "list": "function Main(html){return jsoups(html,'div.epsbox > a');}",
        "name": "function Main(text){return jsoup(text,'a');}",
        "link": "function Main(text){return jsoup(text,'a@attr->href');}",
        "orderBy": 0,
        "category": 2
      }
    }
  },
  "content": {
    "备注": "直接jsoup",
    "list": "function Main(html){var imgs = jsoups(html,'#manga-imgs > div.manga-c');if(imgs[0].length<5){imgs[0]=jsoup(text,'#manga-imgs > div:nth-child(2) > img@attr->src');};return imgs;}",
    "link": "function Main(text){return jsoup(text,'img@attr->data-original');}",
    "mode": 1
  }
}