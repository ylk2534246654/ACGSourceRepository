{
  "name": "多多漫画",
  "introduction": "可以搜索一些漫画",
  "developer": "雨夏",
  "mail":"2534246654@qq.com",
  "versionName": "1.0",
  "versionCode": 1,
  "tab":"漫画",
  "request": "https://api.duoduomh.1a3.net/app2/comic/search?sort=click&page=[$page]&keywords=[$key]@charset->UTF-8",
  "search": {
    "list": "function Main(html){return jsonPaths(html,'$.items.*');}",
    "title": "function Main(text){return jsonPath(text,'$.name');}",
    "introduction": "function Main(text){return jsonPath(text,'$.author')+'\\n'+jsonPath(text,'$.last_chapter_name');}",
    "cover": "function Main(text){return jsonPath(text,'$.coverUrl');}",
    "url": "function Main(text){return 'https://api.duoduomh.1a3.net/app2/comic/view?id='+jsonPath(text,'$.id');}"
  },
  "detail": {
    "author": "function Main(html){return jsoup(html,'div.info > p:nth-child(3)').replace('作者：','');}",
    "introduction": "function Main(html){return jsoup(html,'div.detailContent > p');}",
    "upDate": "function Main(html){return jsoup(html,'p.bottom.d-nowrap');}",
    "tabs": {
      "list": "function Main(html){return jsoups(html,'body > div.chapterList.d-border');}",
      "name": "function Main(text){return jsoup(text,'li.ib.active');}"
    },
    "catalogs": {
      "list": "function Main(html){return jsoups(html,'body > div.chapterList.d-border');}",
      "chapter": {
        "list": "function Main(html){return jsoups(html,'#chapterList_1 > ul > li');}",
        "name": "function Main(text){return jsoup(text,'a');}",
        "url": "function Main(text){return jsoup(text,'a@attr->href');}",
        "category": 2
      }
    }
  },
  "comic": {
    "list": "function Main(html){var file=strmiddle(html,'pl = \\'','/000');var startid=parseInt(strmiddle(html,'/000','.jpg'));var endid=parseInt(strmiddle(html,'pcount=',';'));var obj= [];for(var i =startid;i<(endid+startid);i++){var num = i;var n=4;var a = (Array(n).join(0) + num).slice(-n);obj.push(file+'/'+a+'.jpg');};return obj;}",
    "url": "function Main(text){return text;}",
    "mode": 3
  }
}