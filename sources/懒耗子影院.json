        "introduction": "",
        "upDate": "",
        "cover": "function Main(html){return jsonPath(html,'$.msg.pic');}",
        "catalogs": {
            "chapter": {
                "list": "var mhtml;function Main(html){mhtml = html;return jsonPaths(html,'$.msg.url[*]');}",
                "name": "function Main(text){var name = jsonPath(text,'$.playName');if(name.length<3){return '在线播放';}else{return 'mhtml'+name;}}",
