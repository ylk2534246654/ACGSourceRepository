    "author": "function Main(text){return jsonPath(text,'$.author_name');}",
    "img": "function Main(text){return jsonPath(text,'$.vertical_cover');}",
    "link": "function Main(text){return 'https://manga.bilibili.com/twirp/comic.v2.Comic/ComicDetail?device=pc&platform=web@post->'+'{\"comic_id\":'+jsonPath(text,'$.id')+'}@header->Content-Type:application/json;charset=UTF-8';}"
  },
  "detail": {
    "author": "function Main(html){return jsonPath(html,'$..author_name');}",
    "summary": "function Main(html){return jsonPath(html,'$..evaluate');}",
    "upDate": [],
    "catalog_link": [],
    "tab": {
      "list": [],
      "name": []
    },
    "catalog": {
      "list": [],
      "chapter": {
        "list": "var url = 'https://manga.bilibili.com/m/'+jsonPath(html,'$..data.id');function Main(html){return jsonPaths(html,'$..ep_list[*]');}",
        "name": "function Main(text){return jsonPath(text,'$..title');}",
        "link": "function Main(text){return url+jsonPath(text,'$..id');}",
