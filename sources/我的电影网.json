{
  "name": "我的电影网",
  "charset": "UTF-8",
  "search": {
    "requestURL": "http://www.wodedy.net/search.php",
	  "requestMethod": "POST",
    "requestData": "searchword=[$key]"
  },
  "metadata": {
    "list": "{selector:div.index-area > ul > li}",
    "name": "{selector:a@attr->title}",
    "author": "{selector:p.other}",
    "img": "{selector:a > img@attr->data-original}",
	"link": "{selector:a@attr->href}"
  },
  "detail": {
    "author": "{selector:dl > dt:nth-child(3) > a}",
    "summary": "{selector:div.mb > div > div}",
    "upDate": "{selector:dl > dd:nth-child(6) > a}",
    "catalog_link": [],
    "tab":{
            "list":"{selector:div.playfrom > ul >li}",
            "name":"{selector:li}"
        },
    "catalog":{
        "list":"{selector:#vlink_1}",
        "chapter": {
                "list": "{selector:#vlink_1 > ul > li}",
                "name": "{selector:a}",
                "link": "{selector:a@attr->href}",
                "orderBy": 1,
                "category":3
        }
        
    }
  }
}