        "img": "function Main(html){return jsoup(html,'a.ImgA > img@attr->src');}",
        "link": "function Main(html){return jsoup(html,'a.ImgA@attr->href');}"
    },
    "detail": {
        "author": "function Main(html){return jsoup(html,'div.sub_r > p:nth-child(2)');}",
        "summary": "function Main(html){return jsoup(html,'div.sub_r > p:nth-child(2).replace('介绍:','')');}",
        "upDate": "function Main(html){return jsoup(html,'span.date');}",
        "catalog_link": [],
        "catalog": {
            "list": [],
            "chapter": {
                "list": "function Main(html){return jsoups(html,'div#list > ul > li');}",
                "name": "function Main(html){return jsoup(html,'a');}",
                "link": "function Main(html){return jsoup(html,'a@attr->href');}",
                "orderBy": 0,
                "category": 2
            }
        }
    },
    "content2": {
        "list": "[$url]?p=[$id]",
        "link": "{selector:#qTcms_pic@attr->src}",
        "startid": "1",
        "endid": "{selector:#k_total}",
        "mode": 1
    },
    "content": {
