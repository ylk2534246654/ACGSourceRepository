        "requestURL": "http://m.ccshwy.com/statics/search.aspx?key=[$key]",
        "requestMethod": "GET"
    },
    "metadata": {
        "list": "function Main(html){return jsoups(html,'#listbody > li');}",
        "name": "function Main(html){return jsoup(html,'a.txtA');}",
