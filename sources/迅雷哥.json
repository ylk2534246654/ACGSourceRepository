{
	"name": "迅雷哥",
	"charset": "gb2312",
	"search": {
		"requestURL": "https://m.4142.cc/search.asp",
		"requestMethod": "POST",
		"requestData": "searchword=[$key]"
	},
	"metadata": {
		"list": "{selector:div > ul > li}",
		"name": "{selector:a@attr->title}",
		"author": "",
		"img": "{selector:a > img@attr->src}",
		"link": "{selector:a@attr->href}"
	},
	"detail": {
		"author": "{selector:div.intro > div:nth-child(4) > p:nth-child(2) > a}",
		"summary": "{selector:div.description}",
		"upDate": "{selector:p:nth-child(1) > font}",
		"catalog_link": [],
		"tab": {
			"list": "{selector:body > div > h5}{re:影片介绍,}",
			"name": "{selector:h5}"
		},
		"catalog": {
			"list": "{selector:body > div > div.playlist}",
			"chapter": {
				"list": "{selector:div.playlist > ul > li}",
				"name": "{selector:a@attr->title}",
				"link": "{selector:a@attr->href}",
				"orderBy": 1,
				"category": 3
			}
		}
	}
}