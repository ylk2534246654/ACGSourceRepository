{
  "name": "MangaNel",
  "charset": "UTF-8",
  "search": {
    "requestURL": "https://manganelo.com/search/[$key]",
	"requestMethod": "GET",
    "requestData": ""
  },
  "metadata": {
    "list": "{selector:div.panel-search-story > div}",
    "name": "{selector:a > img@attr->alt}",
    "author": "{selector:span.item-author@attr->title}",
    "img": "{selector:a > img@attr->src}",
	"link": "{selector:a@attr->href}"

  },
  "detail": {
    "author": "{selector:tr:nth-child(2) > td.table-value > a}",
    "summary": "{selector:#panel-story-info-description}",
    "upDate": "{selector:tr:nth-child(3) > td.table-value} Â· {selector:span.stre-value}",
    "catalog_link": [],
    "tab":{
            "list":"{selector:div.panel-story-chapter-list > p}",
            "name":"{selector:span.row-title-chapter-name}"
        },
    "catalog":{
        "list":"{selector:div.panel-story-chapter-list > ul}",
        "chapter": {
                "list": "{selector:ul.row-content-chapter > li}",
		        "name": "{selector:a}",
		        "link": "{selector:a@attr->href}",
                "orderBy": 0,
		        "category":2
        }
    }
  },
  "content": {
    "list":"{selector:#vungdoc > img}",
	"link":"{selector:img@attr->src}",
	"endid":"",
    "mode":1
  }
}