{
  "name": "网易云音乐",
  "charset": "utf-8",
  "search": {
    "requestURL": "https://music.liuzhijin.cn/",
    "requestMethod": "POST",
    "requestHeaders": "X-Requested-With=XMLHttpRequest&User-Agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.130 Safari/537.36",
    "requestData": "input=[$key]&filter=name&type=netease&page=[$page]"
  },
  "metadata": {
    "list": "{selector:data}",
    "name": "{selector:title}",
    "author": "{selector:author}",
    "img": "{selector:pic}",
    "link": "{selector:url}"
  },
  "detail": {
    "author": "作者无法获取",
    "summary": "{selector:#app > div:nth-child(3) > p}",
    "upDate": "{selector:#app > div.episode > div}{sub:[$this],剧集（,）,1}",
    "catalog_link": [],
    "tab": {
      "list": "{selector:#app > div.episode}",
      "name": "{selector:div.title}"
    },
    "catalog": {
      "list": "{selector:#app > div.episode}",
      "chapter": {
        "list": "{selector:div.episode > ul > li}",
        "name": "{selector:a}",
        "link": "{selector:a@attr->href}",
        "orderBy": 1,
        "category": 3
      }
    }
  }
}