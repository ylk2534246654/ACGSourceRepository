{
    "name": "MyGalgame",
    "introduction": "可以搜索一些GalGame",
    "developer": "雨夏",
    "versionName": "1.0",
    "versionCode": 1,
    "request": "https://www.mmgal.com/?s=[$key]@charset->UTF-8",
    "metadata": {
        "list": "function Main(html){return jsoups(html,'#article-list > div');}",
        "title": "function Main(text){return jsoup(text,'div.title-article > h1 > a');}",
        "introduction": "function Main(text){return jsoup(text,'section > center');}",
        "cover": "function Main(text){return jsoup(text,'a > img');}",
        "url": "function Main(text){return jsoup(text,'div.title-article > h1 > a@attr->href');}"
    }
}