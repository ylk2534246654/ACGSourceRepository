{
    "name": "动漫之家2",
    "charset": "UTF-8",
    "search": {
        "requestURL": "https://m.dmzj.com/search/[$key].html",
        "requestMethod": "GET",
        "requestData": ""
    },
    "metadata": {
        "list": "function Main(html){return jsonPaths(strmiddle(html,'serchArry=','</script>'),'$.*');}",
        "name": "function Main(html){return jsonPath(html,'$.name');}",
        "author": "function Main(html){return jsonPath(html,'$.authors');}",
        "img": "function Main(html){return 'https://images.dmzj.com/'+jsonPath(html,'$.cover');}",
        "link": "function Main(html){return 'https://m.dmzj.com/info/'+jsonPath(html,'$.comic_py')+'.html';}"
    },
    "detail": {
        "author": "function Main(html){return jsoup(html,'p.txtItme:nth-child(1)');}",
        "summary": "function Main(html){return jsoup(html,'p.txtDesc.autoHeight').replace('介绍:','');}",
        "upDate": "function Main(html){return jsoup(html,'p:nth-child(3) > a:nth-child(4)')+'-'+jsoup(html,'p:nth-child(4) > span.date');}",
        "catalog_link": [],
        "tab": {
            "list": "function Main(html){return jsoups(html,'#list > div');}",
            "name": "function Main(html){return jsoups(html,'span');}"
        },
        "catalog": {
            "list": [],
            "chapter": {
                "list": "function Main(html){return jsonPaths(strmiddle(html,'initIntroData([',']'),'$.data');}",
                "name": "function Main(html){return jsonPath(html,'$.chapter_name');}",
                "link": "function Main(html){return 'https://m.dmzj.com/view/'+jsonPath(html,'$.comic_id')+'/'+jsonPath(html,'$.id');}+'.html'",
                "orderBy": 0,
                "category": 2
            }
        }
    },
    "content": {
        "备注": "此处爬取数据中的指定json数据,再json解析出link",
        "list": "{sub:[$html],initData(,);,0}{selector:page_url}",
        "link": "{selector:}",
        "mode": 2
    }
}