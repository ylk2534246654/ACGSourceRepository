{
    "name": "动漫之家2",
    "charset": "UTF-8",
    "search": {
        "requestURL": "https://m.dmzj.com/search/[$key].html",
        "requestMethod": "GET",
        "requestData": ""
    },
    "metadata": {
        "list": "function Main(html){return jsonPaths('{\"main\":'+strmiddle(html,'serchArry=','</script>')+'}','$..main');}",
        "name": "function Main(this){return jsonPath(this,'$.name');}",
        "author": "function Main(this){return jsonPath(this,'$.authors');}",
        "img": "function Main(this){return 'https://images.dmzj.com/'+jsonPath(this,'$.cover');}",
        "link": "function Main(this){return 'https://m.dmzj.com/info/'+jsonPath(this,'$.comic_py')+'.html';}"
    },
    "detail": {
        "author": "{selector:p.txtItme:nth-child(1)}",
        "summary": "{selector:p.txtDesc.autoHeight}{re:介绍:, }",
        "upDate": "{selector:p:nth-child(3) > a:nth-child(4)}·{selector:p:nth-child(4) > span.date}",
        "catalog_link": [],
        "tab": {
            "list": "{selector:#list > div}",
            "name": "{selector:span}"
        },
        "catalog": {
            "list": [],
            "chapter": {
                "list": "{sub:[$html],initIntroData([,]),0}{selector:data}",
                "name": "{selector:chapter_name}",
                "link": "https://m.dmzj.com/view/{selector:comic_id}/{selector:id}.html",
                "orderBy": 0,
                "category": 2
            }
        }
    },
    "content": {
        "备注": "此处爬取数据中的指定json数据,再json解析出link",
        "list": "{sub:[$html],initData(,);,0}{selector:page_url}",
        "link": "{selector:}",
        "mode": 2
    }
}