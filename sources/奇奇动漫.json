{
  "name": "奇奇动漫",
  "charset": "UTF-8",
  "search": {
    "requestURL": "http://m.qiqidongman.com/vod-search-wd-[$key].html",
	  "requestMethod": "GET",
    "requestData": ""
  },
  "metadata": {
    "list": "{selector:#LIST > li}",
    "name": "{selector:i.tit}",
    "author": "无法获取",
    "img": "{selector:img@attr->data-src}",
	"link": "{selector:a@attr->href}"
  },
  "detail": {
    "author": "",
    "summary": "{selector:#descCont}",
    "upDate": "{selector:div.contWrap > p:nth-child(2)}",
    "catalog_link": [],
    "tab":{
            "list":"{selector:#playList > div.caption.captionUrlList}",
            "name":"{selector:div}"
        },
    "catalog":{
        "list":"{selector:#playList > div.tv-list}",
        "chapter": {
                "list": "{selector:ul > li}",
                "name": "{selector:a@attr->title}",
                "link": "{selector:a@attr->href}",
                "orderBy": 1,
                "category":3
        }
        
    }
  }
}