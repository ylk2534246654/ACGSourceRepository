{
    "name": "喵绅士",
    "charset": "UTF-8",
    "search": {
        "获取最新节点":"https://hcomicurl.xyz/",
        "requestURL": "https://m.c-upp.com/search/",
        "requestMethod": "POST",
        "requestData": "show=title%2Ctitleen%2Ctags&keyboard=[$key]"
    },
    "metadata": {
        "list": "{selector:div > ul}",
        "name": "{selector:h3}",
        "author": "{selector:a > p}",
        "img": "{selector:img@attr->src}",
        "link": "{selector:a@attr->href}"
    },
    "detail": {
        "author": "",
        "summary": "",
        "upDate": "",
        "catalog_link": [],
        "catalog": {
            "chapter": {
                "list": "{selector:div > a:nth-child(7)}",
                "name": "{selector:a}",
                "link": "{selector:a@attr->href}",
                "orderBy": 0,
                "category": 2
            }
        }
    },
    "content": {
        "备注": "直接jsoup",
        "list": "{selector:div > img}",
        "link": "{selector:img@attr->data-original}",
        "mode": 1
    }
}