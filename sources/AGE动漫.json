{
  "name": "AGE动漫",
  "charset": "UTF-8",
  "search": {
    "requestURL": "https://www.agefans.tv/search?&query=[$key]&page=[$page]",
    "requestMethod": "GET",
    "requestData": ""
  },
  "metadata": {
    "list": "{selector:div[class=blockcontent1] > div}",
    "name": "{selector:a[class=cell_poster] > img@attr->alt}",
    "author": "{selector:div:nth-child(7) > span.cell_imform_value}",
    "img": "{selector:a[class=cell_poster] > img@attr->src}",
    "link": "{selector:a[class=cell_poster]@attr->href}"
  },
  "detail": {
    "author": "{selector:li:nth-child(5) > span.detail_imform_value}",
    "summary": "{selector:div.detail_imform_desc_pre > p}",
    "upDate": "{selector:li:nth-child(8) > span.detail_imform_value}",
    "catalog_link": [],
    "tab": {
      "list": "{selector:#menu0 > li}",
      "name": "{selector:li}"
    },
    "catalog": {
      "list": "{selector:#main0 > div.movurl}",
      "chapter": {
        "list": "{selector:ul > li}",
        "name": "{selector:a}",
        "link": "{selector:a@attr->href}",
        "orderBy": 1,
        "category": 3
      }
    }
  }
}