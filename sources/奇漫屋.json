{
    "name": "奇漫屋",
    "charset": "UTF-8",
    "search": {
        "requestURL": "http://qiman5.com/spotlight/?keyword=[$key]",
        "requestMethod": "GET"
    },
    "metadata": {
        "list": "{selector:div.search-result > div}",
        "name": "{selector:p.comic-name > a}",
        "author": "{selector:p.comic-tags}\n{selector:p.comic-update-at}",
        "img": "{selector:a.cover > img@attr->src}",
        "link": "{selector:p.comic-name > a@attr->href}"
    },
    "detail": {
        "author": "{selector:div.sub_r > p:nth-child(2)}",
        "summary": "{selector:p.txtDesc}{re:介绍:,}",
        "upDate": "{selector:span.date}",
        "catalog_link": [],
        "catalog": {
            "list": [],
            "chapter": {
                "list": "{selector:div#list > ul > li}",
                "name": "{selector:a}",
                "link": "{selector:a@attr->href}",
                "orderBy": 0,
                "category": 2
            }
        }
    },
    "content2": {
        "list": "[$url]?p=[$id]",
        "link": "{selector:#qTcms_pic@attr->src}",
        "startid": "1",
        "endid": "{selector:#k_total}",
        "mode": 1
    },
    "content": {
        "备注": "此处爬取数据中的指定key,再解密",
        "list": "{sub:[$html],m_murl_e=\",\",0}{decode:[$this],Base64}{split:\\$qingtiandy\\$}",
        "link": "{selector:}{re:m.ccshwy.com,www.ccshwy.com}",
        "mode": 1
    }
}